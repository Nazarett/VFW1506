



var win = Ti.UI.createWindow({
	backgroundColor: "#f2f2f2",	
	layout: "horizontal"
});


var title = Ti.UI.createLabel({
	text: "Gallery",
	color: "orange",
	borderRadius: 10,
	backgroundColor "#0a0a0a",
	borderColor: "#0a0a0a",
	borderWidth: 5,
	font: { fontSize: 40, fontFamily: "Times Roman", fontWeight: "bold", fontStyle: "italic"},
	top: 200,
	left: 100
	
});

win.add(title);
win.open();