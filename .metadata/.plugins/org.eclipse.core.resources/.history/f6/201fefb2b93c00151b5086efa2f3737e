Ti.UI.setBackgroundColor("#000");

var mainScreen = Ti.UI.createWindow({
	backroundColor:"#fff",
	backroundImage:"KS_nav_ui.png"
	
});

var myFirstLook = Ti.UI.createView({
	backroundColor:(#333"),
	borderRadius:5,
	borderWidth:1,
	width:100,
	height:100
});

 var mainScreen.add(myFirstLook);
mainScreen.open();
