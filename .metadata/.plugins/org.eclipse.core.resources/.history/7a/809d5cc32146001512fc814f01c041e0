var win = Ti.UI.createWindow({
	backgroundColor: "#f2f2f2",
	layout: "vertical"
});

var myPics = ["destiny.jpg"];

console.log(myPics);

var thePics = Ti.UI.createImageView({
	image: "pics/destiny.jpg",
	width:"50%",
	top: 100	
});

win.add(thePics);

win.open();


