Ti.UI.setBackgroundColor("#000");

var mainScreen = Ti.UI.createWindow({
	title: "Main screen", //Only for android
	backgroundColor: "#fff"
	
});
var myFirstLook = Ti.UI.createView({
	backgroundColor:"orange",
	borderRadius:5,
	borderWidth:1,
	width:100,
	height:50
});
mainScreen.add(myFirstLook);

mainScreen.open();
