
var pics = Ti.UI.createWindow({
	backgroundColor: "#f2f2f2",
	layout: "vertical"
});

var collage = Ti.Filesystem.getFile(Ti.Filesystem.resourcesDirectory,"pics");

var thePics = myPics.getDirectoryListing();

console.log(thePics);

for(var i= 0; i<thePics.length; i++){
	var myPics =Ti.UI.createImagesView({
		image: "pics" + myPics[i],
		width:"50%",
		top: 30
	});
	
	pics.add(myPics);
}




pics.open();




